// src/framer-components/MyFramerComponent.tsx
import { motion } from "framer-motion";

export function MyFramerComponent() {
  return (
    <motion.div
      initial={{ opacity: 0 }}
      animate={{ opacity: 1 }}
      transition={{ duration: 0.5 }}
    >
      <h1>Hello from Framer!</h1>
    </motion.div>
  );
}
